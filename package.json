{
  "name": "bitkeeper-server-js",
  "version": "0.0.1",
  "description": "JS web server for Tradle bitkeeper component",
  "main": "index.js",
  "bin": {
    "bitkeeper": "./app.js"
  },
  "scripts": {
    "postinstall": "cp node_modules/tradle-style/.js* .",
    "jsbeautify": "js-beautify -r {*.js,**/*.js}",
    "jscs": "jscs {*.js,**/*.js}",
    "jshint": "jshint {*.js,**/*.js}",
    "run": "node ./app.js",
    "testrun": "node ./app.js --dht=false"
  },
  "pre-commit": [
    "jscs",
    "jshint"
  ],
  "bugs": {
    "url": "https://github.com/tradle/bitkeeper-server-js/issues"
  },
  "homepage": "https://github.com/tradle/bitkeeper-server-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bitkeeper-server-js.git"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.10.1",
    "concat-stream": "^1.4.7",
    "debug": "^2.1.1",
    "express": "^4.10.7",
    "minimist": "^1.1.0",
    "bitkeeper-js": "git://github.com/tradle/bitkeeper-js.git",
    "tradle-utils": "git://github.com/tradle/tradle-utils.git",
    "promise-ports": "git://github.com/tradle/promise-ports.git"
  },
  "devDependencies": {
    "bittorrent-dht": "^2.6.4",
    "js-beautify": "latest",
    "jscs": "^1.10.0",
    "jshint": "latest",
    "pre-commit": "0.0.11",
    "tape": "^3.4.0",
    "tradle-style": "0.0.1"
  }
}
